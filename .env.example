# URL de tu endpoint para sincronizar el catálogo entre dispositivos
VITE_CATALOG_SYNC_URL=/.netlify/functions/catalog-sync

# Opcional: método HTTP para guardar (PUT | POST | PATCH)
VITE_CATALOG_SYNC_METHOD=PUT

# Opcional: token para autenticación
VITE_CATALOG_SYNC_TOKEN=

# Opcional: header para el token (Authorization, x-api-key, etc)
VITE_CATALOG_SYNC_TOKEN_HEADER=Authorization
